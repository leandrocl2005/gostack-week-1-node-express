{
  "plugins": ["prettier"],
  "extends": "prettier",
  "rules": {
    "prettier/prettier": "error"
  },
  "env": {
    "browser": true,
    "es6": true,
    "jest": true
  },
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },

  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  }
}
